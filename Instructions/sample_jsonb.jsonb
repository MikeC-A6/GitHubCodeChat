    "content": "import { SimpleGit, simpleGit } from 'simple-git';\nimport fs from 'fs/promises';\nimport path from 'path';\nimport os from 'os';\nimport { getPatterns } from './patterns.js';\nimport { minimatch } from 'minimatch';\n\n// Rest of the original file content...\n"
